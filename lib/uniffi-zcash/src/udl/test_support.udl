
/*
	outputs: record<DOMString, sequence<u8>>
	args: record<DOMString, sequence<string>>
*/
interface ZcashTestSupport {
    [Name=from_csv_file]//, Throws=ZcashError]
    constructor();

    sequence<u8> get_output_as_bytes([ByRef] string method);

    string get_argument_as_string([ByRef] string method, u32 idx);
    u32 get_argument_as_integer([ByRef] string method, u32 idx);
    sequence<u8> get_argument_as_byte_array([ByRef] string method, u32 idx);
};

/*
	method: String,
	output: String,
	args: String,
*/
interface ZcashSpecificTestSupport {
    [Name=from_method]//, Throws=ZcashError]
    constructor([ByRef] string method);

    sequence<u8> get_output_as_bytes();

    string get_argument_as_string(u32 idx);
    u32 get_argument_as_integer(u32 idx);
    sequence<u8> get_argument_as_byte_array(u32 idx);
};
